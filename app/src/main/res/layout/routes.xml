<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/match_parent">

    <!-- Navbar Fragment -->
    <fragment android:name="edu.calvin.cs262.climbinglogapp.NavBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/NavBarHeight"
        android:layout_marginBottom="350dp"
        android:layout_alignParentStart="true"
        android:id="@+id/fragment2" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:id="@+id/get_routes_text"
        android:layout_marginRight="15dp"
        android:layout_above="@+id/get_routes_button"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="128dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GET"
        android:id="@+id/get_routes_button"
        android:layout_below="@+id/fragment2"
        android:layout_centerHorizontal="true" />

</RelativeLayout>